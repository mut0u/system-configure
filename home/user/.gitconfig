[user]
	name = savior
	email = michael.savior@gmail.com
[alias]
	br = branch
	co = checkout
	st = status
[push]
	default = simple
[core]
	editor = vim

[mergetool "ediff"]
	cmd = emacsclient --eval \"(git-mergetool-emacsclient-ediff \\\"$LOCAL\\\" \\\"$REMOTE\\\" \\\"$BASE\\\" \\\"$MERGED\\\")\"
	trustExitCode = false
[mergetool]
	prompt = false
[merge]
	tool = ediff

